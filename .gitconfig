[user]
    name = Helmor Ritualo
    email = helmorritualo@gmail.com

[core]
    editor = code --wait
    autocrlf = input
    whitespace = trailing-space,space-before-tab

[color]
    ui = auto

[push]
    default = current

[pull]
    rebase = false

[init]
    defaultBranch = main

[alias]
    s = status
    a = add .
    c = commit -m
    p = push
    pl = pull
    co = checkout
    cb = checkout -b
    log = log --oneline --graph --decorate
    undo = reset --soft HEAD~1

[credential]
    helper = store
